@model StudentViewModel

@{
    ViewData["Title"] = "Create";
}

<h2>Create Student</h2>

<form asp-action="Create" method="post">
    @Html.LabelFor(m => m.StudentInfo.Id)
    @Html.TextBoxFor(m => m.StudentInfo.Id, new { @class = "form-control", @required = "true" })
    @Html.ValidationMessageFor(m => m.StudentInfo.Id, "", new { @class = "text-danger" })
    <br />

    <partial name="_StudentFormFields" model="Model" />
</form>