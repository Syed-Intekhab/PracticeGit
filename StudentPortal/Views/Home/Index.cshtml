@model List<Student>

@{
    ViewData["Title"] = "Home";
}

<style>
    table,
    th,
    td{
        border: 1px solid black;
        border-collapse: collapse;
    }
</style>

<table border="1" cellpadding="10" cellspacing="10">
    <tr>
        <th>Id</th>
        <th>Name</th>
        <th>Email</th>
        <th>Course</th>
    </tr>
    
    @foreach (Student student in Model)
    {
    <tr>
        <td>@student.Id</td>
        <td>@student.Name</td>
        <td>@student.Email</td>
        <td>@student.Course</td>
    </tr>
    }
    
</table>